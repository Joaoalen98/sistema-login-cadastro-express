// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  // exemplo de conex√£o a um banco de dados realcional
  provider = "mysql"
  url      = env("DATABASE_URL")
}


//schema da tabela
model Usuario {
  id String @id @default(uuid())
  nome String
  email String
  senha String

  @@map("usuarios")
}